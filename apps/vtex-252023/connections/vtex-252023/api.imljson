{
    // https://developers.vtex.com/vtex-rest-api/docs/getting-started-authentication
    "url": "https://{{parameters.accountName}}.{{parameters.environment}}.com.br/api/vlm/account",
    "headers": {
        "X-VTEX-API-AppKey": "{{parameters.appKey}}",
        "X-VTEX-API-AppToken": "{{parameters.appToken}}"
    },
    "log": {
        "sanitize": [
            "request.headers.X-VTEX-API-AppKey",
            "request.headers.X-VTEX-API-AppToken"
        ]
    }
}